{
    "name": "elastic-search-builder",
    "version": "0.1.0",
    "description": "An elasticsearch search option builder.",
    "main": "dist/index.js",
    "files": [
        "dist/",
        "src/"
    ],
    "scripts": {
        "build:docs": "npm run build:md && documentation build src/index.js -o docs -f html",
        "build:md": "documentation build src/index.js -o docs/API.md -f md",
        "build": "npm run lint && npm run test && babel src -d dist",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "release": "npm run build && npm publish",
        "test": "jest",
        "test:coverage": "jest --coverage --runInBand --forceExit",
        "test:watch": "jest --watch"
    },
    "author": {
        "name": "weihanchen",
        "email": "willhanchen@gmail.com"
    },
    "contributors": [],
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "documentation": "^5.3.3",
        "eslint": "^4.12.1",
        "jest": "^21.2.1"
    },
    "dependencies": {
        "update-notifier": "^2.3.0"
    },
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:weihanchen/elastic-search-builder.git"
    },
    "keywords": [
        "elasticsearch",
        "querybuilder",
        "querying",
        "queries",
        "query",
        "elastic",
        "search",
        "dsl"
    ]
}
